{
  "name": "styled-system-scale",
  "version": "0.1.0",
  "description": "Assign scales to styled-system props",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/styled-system-scale",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/styled-system-scale.git"
  },
  "keywords": [
    "react",
    "styled-system",
    "styled-components",
    "emotion"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "styled-system": "^4"
  },
  "peerDependencies": {
    "styled-system": "4.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
