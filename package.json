{
  "name": "styled-system-scale",
  "version": "0.4.1",
  "description": "Assign scales to styled-system props",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/styled-system-scale",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/styled-system-scale.git"
  },
  "keywords": [
    "react",
    "styled-system",
    "styled-components",
    "emotion"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "emotion-theming": "^10.0.14",
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "standard-version": "^6.0.1",
    "styled-system": "^5.0.12"
  },
  "peerDependencies": {
    "styled-system": ">= 4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "polished": "^3.4.1"
  }
}
